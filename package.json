{
	"name": "onboarding",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"author": {
		"name": "Irfanul Islam",
		"email": "irfanulislamdu@gmail.com"
	},
	"scripts": {
		"dev": "npm-run-all --parallel vite:dev typesafe-i18n",
		"vite:dev": "vite dev --host --port 5176",
		"build": "vite build",
		"start": "node build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write . && eslint . --fix",
		"prepare": "husky install && svelte-kit sync",
		"configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
		"typesafe-i18n": "typesafe-i18n",
		"update-deps": "taze major -I",
		"story:dev": "histoire dev",
		"story:build": "histoire build",
		"story:preview": "histoire preview"
	},
	"devDependencies": {
		"@csstools/postcss-media-minmax": "^2.0.1",
		"@fingerprintjs/fingerprintjs": "^4.5.0",
		"@histoire/plugin-svelte": "^0.17.17",
		"@playwright/test": "^1.47.1",
		"@shamscorner/config-eslint": "^0.0.3",
		"@shamscorner/config-tailwindcss": "^0.0.4",
		"@shamscorner/shared": "^0.0.2",
		"@shamscorner/svelte-shared": "^0.0.2",
		"@sveltejs/adapter-auto": "^3.2.5",
		"@sveltejs/adapter-vercel": "^5.4.4",
		"@sveltejs/kit": "^2.17.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@types/gtag.js": "^0.0.20",
		"@vercel/analytics": "^1.3.1",
		"@vercel/speed-insights": "^1.0.12",
		"@zerodevx/svelte-img": "^2.1.1",
		"autoprefixer": "^10.4.20",
		"fontaine": "^0.5.0",
		"histoire": "^0.17.17",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"lottie-web": "^5.12.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"resend": "^4.0.0",
		"svelte": "^4.2.19",
		"svelte-check": "^4.0.2",
		"svelte-confetti": "^1.0.0",
		"sveltekit-superforms": "^2.18.1",
		"tailwindcss": "^3.4.12",
		"taze": "^0.16.9",
		"tslib": "^2.7.0",
		"typesafe-i18n": "^5.26.2",
		"typescript": "^5.6.2",
		"vite": "^5.4.14",
		"vitest": "^2.1.1",
		"zod": "^3.23.8"
	},
	"lint-staged": {
		"**/*.{js,ts,jsx,json,svelte}": [
			"eslint . --fix",
			"prettier --write ."
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@builder.io/partytown": "^0.10.2",
		"bits-ui": "^0.21.13",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"formsnap": "^1.0.1",
		"lucide-svelte": "^0.441.0",
		"sharp": "^0.33.5",
		"tailwind-merge": "^2.5.2",
		"tailwind-variants": "^0.2.1"
	}
}
